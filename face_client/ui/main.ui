                    <string notr="true">/* Main Window */
QMainWindow {
    background-color: #2E3440;
    color: #ECEFF4;
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
    font-size: 12pt;
}

/* Labels */
QLabel {
    color: #ECEFF4;
    font-size: 12pt;
    padding: 5px;
}

/* Table View */
QTableView {
    background-color: #3B4252;
    color: #ECEFF4;
    gridline-color: #4C566A;
    border: 1px solid #4C566A;
}

QHeaderView::section {
    background-color: #4C566A;
    color: #ECEFF4;
    padding: 5px;
    border: 1px solid #4C566A;
}

QTableView::item {
    background-color: #3B4252;
    color: #ECEFF4;
    border: 1px solid #4C566A;
}

QTableView::item:selected {
    background-color: #5E81AC;
    color: #ECEFF4;
}

/* Buttons */
QPushButton {
    background-color: #5E81AC;
    color: #ECEFF4;
    border: 1px solid #4C566A;
    padding: 10px;
    border-radius: 5px;
}

QPushButton:hover {
    background-color: #81A1C1;
}

QPushButton:pressed {
    background-color: #4C566A;
}

/* Line Edit */
QLineEdit {
    background-color: #3B4252;
    color: #ECEFF4;
    border: 1px solid #4C566A;
    padding: 5px;
    border-radius: 5px;
}

/* Combo Box */
QComboBox {
    background-color: #3B4252;
    color: #ECEFF4;
    border: 1px solid #4C566A;
    padding: 5px;
    border-radius: 5px;
}

QComboBox QAbstractItemView {
    background-color: #3B4252;
    color: #ECEFF4;
    border: 1px solid #4C566A;
    selection-background-color: #5E81AC;
}

/* Group Box */
QGroupBox {
    background-color: #3B4252;
    color: #ECEFF4;
    border: 1px solid #4C566A;
    border-radius: 5px;
    margin-top: 10px;
    padding: 10px;
}

QGroupBox::title {
    background-color: transparent;
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 0 5px;
}
</string>
